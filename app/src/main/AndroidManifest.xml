    package="com.example.mercer.bluecareer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.LoginActivity" />
        <activity android:name=".Activities.RegistActivity" />
        <activity android:name=".Activities.InfoActivity" />
        <activity android:name=".Activities.MainActivity" />
        <activity android:name=".TestAnswerActivity" />
        <activity android:name=".ClassActivtity" />
